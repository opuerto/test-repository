#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/oscarpuerto/Documents/github/test-repository/tools/my-toolchain/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/tools/my-toolchain/node_modules:/Users/oscarpuerto/Documents/github/test-repository/tools/node_modules:/Users/oscarpuerto/Documents/github/test-repository/node_modules:/Users/oscarpuerto/Documents/github/node_modules:/Users/oscarpuerto/Documents/node_modules:/Users/oscarpuerto/node_modules:/Users/node_modules:/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/my-controls/node_modules/my-toolchain/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/my-controls/node_modules/my-toolchain/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/my-controls/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/oscarpuerto/Documents/github/test-repository/tools/my-toolchain/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/tools/my-toolchain/node_modules:/Users/oscarpuerto/Documents/github/test-repository/tools/node_modules:/Users/oscarpuerto/Documents/github/test-repository/node_modules:/Users/oscarpuerto/Documents/github/node_modules:/Users/oscarpuerto/Documents/node_modules:/Users/oscarpuerto/node_modules:/Users/node_modules:/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/my-controls/node_modules/my-toolchain/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/my-controls/node_modules/my-toolchain/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/my-controls/node_modules:/Users/oscarpuerto/Documents/github/test-repository/libraries/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../my-toolchain/bin/my-build.js" "$@"
else
  exec node  "$basedir/../my-toolchain/bin/my-build.js" "$@"
fi
