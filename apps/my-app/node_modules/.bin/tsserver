#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/typescript@3.9.10/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/node_modules:/Users/oscarpuerto/Documents/github/test-repository/node_modules:/Users/oscarpuerto/Documents/github/node_modules:/Users/oscarpuerto/Documents/node_modules:/Users/oscarpuerto/node_modules:/Users/node_modules:/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/my-app/node_modules/typescript/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/my-app/node_modules/typescript/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/my-app/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/typescript@3.9.10/node_modules/typescript/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/typescript@3.9.10/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules/.pnpm/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/temp/node_modules:/Users/oscarpuerto/Documents/github/test-repository/common/node_modules:/Users/oscarpuerto/Documents/github/test-repository/node_modules:/Users/oscarpuerto/Documents/github/node_modules:/Users/oscarpuerto/Documents/node_modules:/Users/oscarpuerto/node_modules:/Users/node_modules:/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/my-app/node_modules/typescript/bin/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/my-app/node_modules/typescript/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/my-app/node_modules:/Users/oscarpuerto/Documents/github/test-repository/apps/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../typescript/bin/tsserver" "$@"
fi
